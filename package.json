{
  "name": "@fpc/types",
  "description": "Dynamic type checking functions",
  "version": "1.0.0",
  "author": "Emiliano Bovetti <emiliano.bovetti@gmail.com>",
  "license": "GPL-3.0",
  "keywords": [
    "functional",
    "types",
    "fpc",
    "fp"
  ],
  "type": "module",
  "main": "./src/index",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "jest": {
    "transform": {
      "^.*\\.js$": "babel-jest-modules"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@fpc)"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fpc-js/types.git"
  },
  "bugs": {
    "url": "https://github.com/fpc-js/types/issues"
  },
  "release": {
    "branch": "master",
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "src/*.js"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-jest-modules": "^0.0.2",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1"
  }
}
